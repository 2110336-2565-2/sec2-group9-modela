{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["lib:build", "^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test:cov": {
      "dependsOn": ["lib:build"],
      "cache": false
    },
    "lib:build": {
      "cache": true
    }
  }
}
